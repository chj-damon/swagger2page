{
  "publisher": "chjdamon",
  "name": "swagger2page",
  "displayName": "swagger2page",
  "version": "0.0.0",
  "packageManager": "pnpm@8.10.5",
  "description": "a vscode extension to help developers create CRUD pages in umijs/max project based on swagger",
  "author": "chj-damon <https://github.com/chj-damon>",
  "license": "MIT",
  "homepage": "https://github.com/chj-damon/swagger2page#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/chj-damon/swagger2page"
  },
  "bugs": {
    "url": "https://github.com/chj-damon/swagger2page/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "engines": {
    "vscode": "^1.84.2"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "swagger2page-container",
          "title": "swagger2page",
          "icon": "assets/logo.svg"
        }
      ]
    },
    "views": {
      "swagger2page-container": [
        {
          "id": "swagger2page-view",
          "name": "列表",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "dev": "pnpm build --watch --sourcemap",
    "build": "tsup",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "publish": "vsce publish --no-dependencies",
    "tsc": "tsc --noEmit",
    "release": "bumpp && pnpm run publish",
    "web-build": "pnpm --filter web build",
    "web-dev": "pnpm --filter web dev",
    "sidebar-build": "pnpm --filter sidebar build",
    "sidebar-dev": "pnpm --filter sidebar dev"
  },
  "devDependencies": {
    "@simon_he/eslint-config": "^0.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.18.12",
    "@types/vscode": "^1.84.2",
    "@vscode-use/utils": "^0.0.63",
    "bumpp": "^9.2.0",
    "eslint": "^8.54.0",
    "esno": "^0.16.3",
    "lint-staged": "^10.0.7",
    "lodash-es": "^4.17.21",
    "pnpm": "^8.10.5",
    "prettier": "^3.2.4",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.9",
    "rimraf": "^4.4.1",
    "tsup": "^6.7.0",
    "typescript": "^5.3.2",
    "vite": "^4.5.0",
    "vsce": "^2.15.0",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  }
}
